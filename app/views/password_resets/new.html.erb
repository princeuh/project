<% provide(:title, "Forgot password") %>
<div class="content">
<div class="forms" style="height: 300px; margin-top: 120px" >
	<h4 class="contact-header"> RESET PASSWORD </h4>
    <%= form_for(:password_reset, url: password_resets_path) do |f| %>
      <%= f.label :email %><br>
      <%= f.email_field :email %><br>
      <%= f.submit "Submit" %>
    <% end %>
</div>
</div>
