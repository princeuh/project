<div class="forms" id="upload_memo">
<%= form_for(:memo, url: nemabollon_files_path, html: { multipart: true }) do |f| %>
	  <%= f.label :memo_name %><br>
      <%= f.text_field :memo_name %><br>
      <%= f.label :attachment %><br>
      <%= f.file_field :memo %><br>
      <%= f.submit "Save" %>
 <% end %>
</div>