<div class="forms">
<h4 class="contact-header" > CREATE POST  </h4>
<%= form_for(:post, url: posts_path) do |f| %>
	<%= f.label :Title %><br>
	<%= f.text_field :title %><br>
	<%= f.label :Content %><br>
	<%= f.text_area :content %><br>
	<%= f.file_field :picture %> <br>
	<%= f.submit "Upload Post" %>
<% end %>
</div>
